{
  "name": "asana-backend",
  "version": "0.0.0",
  "private": true,
  "author": "Usman H <usman.hasan619@gmail.com>",
  "scripts": {
    "start": "node bin/www.js",
    "dev": "nodemon bin/www.js",
    "debug": "nodemon --inspect bin/www.js",
    "prepare": "husky install",
    "clean": "npm cache clean --force",
    "lint": "eslint .",
    "lint:fix": "./node_modules/.bin/eslint --ignore-path .gitignore . --fix",
    "i": "npm i --legacy-peer-deps"
  },
  "type": "module",
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint:fix"
    }
  },
  "lint-staged": {
    "*.js": [
      "./node_modules/.bin/eslint -- --fix",
      "git add"
    ]
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.451.0",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.1",
    "cookie-parser": "~1.4.6",
    "cors": "^2.8.5",
    "country-timezone-list": "^1.0.5",
    "debug": "~4.3.4",
    "dotenv": "^16.0.3",
    "expo-server-sdk": "^3.7.0",
    "express": "~4.18.2",
    "express-rate-limit": "^7.1.5",
    "gridfs-stream": "^1.1.1",
    "helmet": "^7.0.0",
    "http-errors": "~2.0.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "mongoose": "^6.7.4",
    "mongoose-auto-increment": "^5.0.1",
    "mongose": "0.0.2-security",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.5",
    "nodemon": "^3.0.1",
    "parse-error": "^0.2.0",
    "passport": "^0.6.0",
    "rest-api-errors": "^1.3.1",
    "socket.io": "^4.8.0",
    "toobusy-js": "^0.5.1",
    "twilio": "^4.19.0"
  },
  "devDependencies": {
    "eslint": "^8.49.0",
    "husky": "^8.0.3"
  }
}
